
<urlrewrite>
	<!-- <rule>
		<from>/news</from>
		<to>/tin-tuc</to>
	</rule> -->
	<rule>
		<from>/danh-muc-tin/(.*)-([0-9]+)/pages-([0-9]+).html</from>
		<to>/danh-muc-tin?cid=$2&amp;pages=$3</to>
	</rule>
	<rule>
		<from>/tin-tuc/pages-([0-9]+).html</from>
		<to>/tin-tuc?pages=$1</to>
	</rule>
	<rule>
		<from>/danh-muc-tin/(.*)-([0-9]+).html</from>
		<to>/danh-muc-tin?cid=$2</to>
	</rule>
	<rule>
		<from>/chi-tiet-tin/(.*)-([0-9]+).html</from>
		<to>/chi-tiet-tin?nid=$2</to>
	</rule>
	<rule>
		<from>/admin/user/edit/(.*)-([0-9]+).html</from>
		<to>/admin/user/edit?uid=$2</to>
	</rule>
	<rule>
		<from>/admin/user/delete/(.*)-([0-9]+).html</from>
		<to>/admin/user/delete?uid=$2</to>
	</rule>
	
	<rule>
		<from>/admin/cat/edit/(.*)-([0-9]+).html</from>
		<to>/admin/cat/edit?cid=$2</to>
	</rule>
	<rule>
		<from>/admin/cat/delete/(.*)-([0-9]+).html</from>
		<to>/admin/cat/delete?cid=$2</to>
	</rule>
	
	<rule>
		<from>/admin/news/pages-([0-9]+).html</from>
		<to>/admin/news?pages=$1</to>
	</rule>
	<rule>
		<from>/admin/news/edit/(.*)-([0-9]+).html</from>
		<to>/admin/news/edit?nid=$2</to>
	</rule>
	<rule>
		<from>/admin/news/delete/(.*)-([0-9]+).html</from>
		<to>/admin/news/delete?nid=$2</to>
	</rule>
	
</urlrewrite>